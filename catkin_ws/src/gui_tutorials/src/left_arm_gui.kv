FloatLayout:
    canvas.before:
        Color:
            rgba: 0, 0, 0, 1
        Rectangle:
            pos: self.pos
            size: self.size
    Label:
        text: 'Left Arm GUI'
        size_hint: None, None
        size: 300, 300
        bold: True
        id: test
        pos_hint: {'center_x':0.5, 'center_y':0.97}

    # MOTORS

    Label:
